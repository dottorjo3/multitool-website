{
  "$id": "text-keyword-extract-basic.schema.json",
  "type": "object",
  "properties": {
    "input": {
      "type": "string",
      "minLength": 1,
      "errorMessage": "Inserisci del testo da analizzare"
    },
    "minLength": {
      "type": "string",
      "pattern": "^[1-9][0-9]*$",
      "default": "3"
    },
    "topN": {
      "type": "string",
      "pattern": "^[1-9][0-9]*$",
      "default": "20"
    }
  },
  "required": ["input"],
  "additionalProperties": false
}

