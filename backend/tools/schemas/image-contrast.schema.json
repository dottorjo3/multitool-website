{
  "$id": "image-contrast.schema.json",
  "type": "object",
  "properties": {
    "contrast": {
      "type": ["number", "string"],
      "pattern": "^[0-9]+(\\.[0-9]+)?$",
      "default": "1",
      "errorMessage": "Il contrasto deve essere compreso tra 0.1 e 3"
    },
    "quality": {
      "type": ["integer", "string"],
      "pattern": "^[0-9]{1,3}$",
      "default": "85"
    }
  },
  "required": [],
  "additionalProperties": false
}


