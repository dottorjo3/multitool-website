{
  "$id": "data-json-transform.schema.json",
  "type": "object",
  "properties": {
    "input": {
      "type": "string",
      "minLength": 1,
      "errorMessage": "Inserisci del JSON da trasformare"
    },
    "transformType": {
      "type": "string",
      "enum": ["reverse-array", "uppercase-keys", "lowercase-keys"],
      "default": "reverse-array"
    }
  },
  "required": ["input"],
  "additionalProperties": false
}


