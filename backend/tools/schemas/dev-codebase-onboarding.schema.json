{
  "$id": "dev-codebase-onboarding.schema.json",
  "type": "object",
  "properties": {
    "repoCount": {
      "type": "integer",
      "minimum": 1,
      "maximum": 50,
      "default": 5
    },
    "stack": {
      "type": "string",
      "minLength": 2,
      "default": "TypeScript/Node.js"
    },
    "onboardingDurationDays": {
      "type": "integer",
      "minimum": 3,
      "maximum": 60,
      "default": 14
    }
  },
  "additionalProperties": false
}









