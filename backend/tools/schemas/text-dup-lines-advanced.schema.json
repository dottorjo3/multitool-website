{
  "$id": "text-dup-lines-advanced.schema.json",
  "type": "object",
  "properties": {
    "input": {
      "type": "string",
      "minLength": 1,
      "errorMessage": "Inserisci del testo da analizzare"
    },
    "caseSensitive": {
      "type": "string",
      "enum": ["true", "false"],
      "default": "false"
    },
    "trimWhitespace": {
      "type": "string",
      "enum": ["true", "false"],
      "default": "true"
    }
  },
  "required": ["input"],
  "additionalProperties": false
}

