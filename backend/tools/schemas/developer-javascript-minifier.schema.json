{
  "$id": "developer-javascript-minifier.schema.json",
  "type": "object",
  "properties": {
    "input": {
      "type": "string",
      "minLength": 1,
      "errorMessage": "Inserisci del codice JavaScript da minificare"
    },
    "compress": {
      "type": "string",
      "enum": ["true", "false"],
      "default": "true"
    },
    "mangle": {
      "type": "string",
      "enum": ["true", "false"],
      "default": "true"
    }
  },
  "required": ["input"],
  "additionalProperties": false
}


