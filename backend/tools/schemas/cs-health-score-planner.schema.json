{
  "$id": "cs-health-score-planner.schema.json",
  "type": "object",
  "properties": {
    "dimensions": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 3
      },
      "minItems": 2,
      "uniqueItems": true
    },
    "scoringModel": {
      "type": "string",
      "enum": [
        "weighted",
        "rule-based",
        "ml-based"
      ],
      "default": "weighted"
    },
    "alertThreshold": {
      "type": "integer",
      "minimum": 0,
      "maximum": 100,
      "default": 60
    }
  },
  "additionalProperties": false
}






