{
  "$id": "automation-notification-router.schema.json",
  "type": "object",
  "properties": {
    "channels": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "email",
          "slack",
          "sms",
          "teams"
        ]
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "priorityLevels": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "low",
          "medium",
          "high",
          "critical"
        ]
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "defaultDelaySeconds": {
      "type": "integer",
      "minimum": 0,
      "maximum": 3600,
      "default": 0
    }
  },
  "additionalProperties": false
}









