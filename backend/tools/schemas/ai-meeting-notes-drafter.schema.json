{
  "$id": "ai-meeting-notes-drafter.schema.json",
  "type": "object",
  "properties": {
    "meetingType": {
      "type": "string",
      "minLength": 3,
      "default": "product sync"
    },
    "durationMinutes": {
      "type": "integer",
      "minimum": 5,
      "maximum": 480,
      "default": 45
    },
    "attendees": {
      "type": "integer",
      "minimum": 1,
      "maximum": 50,
      "default": 6
    },
    "includeActionItems": {
      "type": "boolean",
      "default": true
    }
  },
  "additionalProperties": false
}









