{
  "$id": "pdf-version-control.schema.json",
  "type": "object",
  "properties": {
    "storage": {
      "type": "string",
      "enum": [
        "S3",
        "AzureBlob",
        "GCS",
        "OnPrem"
      ],
      "default": "S3"
    },
    "approvalFlow": {
      "type": "string",
      "minLength": 3,
      "default": "legal-review"
    },
    "keepHistory": {
      "type": "integer",
      "minimum": 1,
      "maximum": 100,
      "default": 10
    }
  },
  "additionalProperties": false
}









