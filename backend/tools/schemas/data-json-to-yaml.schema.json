{
  "$id": "data-json-to-yaml.schema.json",
  "type": "object",
  "properties": {
    "input": {
      "type": "string",
      "minLength": 1,
      "errorMessage": "Inserisci del JSON da convertire"
    },
    "indent": {
      "type": "string",
      "pattern": "^[1-9][0-9]*$",
      "default": "2"
    }
  },
  "required": ["input"],
  "additionalProperties": false
}


