{
  "$id": "automation-workflow-simulator.schema.json",
  "type": "object",
  "properties": {
    "steps": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 2
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "concurrency": {
      "type": "integer",
      "minimum": 1,
      "maximum": 100,
      "default": 5
    },
    "failureRate": {
      "type": "number",
      "minimum": 0,
      "maximum": 100,
      "default": 2
    }
  },
  "additionalProperties": false
}









