{
  "$id": "docx-rich-snippets.schema.json",
  "type": "object",
  "properties": {
    "snippetCount": {
      "type": "integer",
      "minimum": 1,
      "maximum": 200,
      "default": 20
    },
    "placeholderSyntax": {
      "type": "string",
      "minLength": 2,
      "default": "{{variable}}"
    },
    "storageLocation": {
      "type": "string",
      "enum": [
        "SharePoint",
        "GoogleDrive",
        "Dropbox",
        "Git"
      ],
      "default": "SharePoint"
    }
  },
  "additionalProperties": false
}









