{
  "$id": "data-csv-remove-column.schema.json",
  "type": "object",
  "properties": {
    "input": {
      "type": "string",
      "minLength": 1,
      "errorMessage": "Inserisci del CSV da modificare"
    },
    "columnName": {
      "type": "string",
      "minLength": 1,
      "errorMessage": "Specifica il nome della colonna da rimuovere"
    },
    "delimiter": {
      "type": "string",
      "default": ","
    }
  },
  "required": ["input", "columnName"],
  "additionalProperties": false
}


