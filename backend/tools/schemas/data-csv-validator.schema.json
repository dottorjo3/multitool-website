{
  "$id": "data-csv-validator.schema.json",
  "type": "object",
  "properties": {
    "input": {
      "type": "string",
      "minLength": 1,
      "errorMessage": "Inserisci del CSV da validare"
    },
    "delimiter": {
      "type": "string",
      "default": ","
    },
    "strictColumnCount": {
      "type": "string",
      "enum": ["true", "false"],
      "default": "false"
    }
  },
  "required": ["input"],
  "additionalProperties": false
}


